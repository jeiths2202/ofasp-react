// REPORT_ASP - Fujitsu ASP Report Generation CL
// Generate system reports

// Start message
MSG "Starting ASP system report generation"

// Generate file listing report
WRKOBJ OBJ(*ALL) OBJTYPE(*FILE) OUTPUT(*PRINT)

// Generate library information
WRKLIB LIB(*ALL) OUTPUT(*PRINT)

// Generate volume information  
WRKVOL VOL(*ALL) OUTPUT(*PRINT)

// Call Java report generator
CALL PGM(EmployeeFileProcessor) PARM("*ALL" "RPT")

// Completion message
MSG "Report generation completed"